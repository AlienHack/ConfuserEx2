{
	"version": "1.4.0+{height}",
	"nugetPackageVersion": {
		"semVer": 1
	},
	"semVer1NumericIdentifierPadding": 4,
	"publicReleaseRefSpec": [
		"^refs/tags/v\\d+\\.\\d+"
	],
	"cloudBuild": {
		"setVersionVariables": true,
		"buildNumber": {
			"enabled": false,
			"includeCommitId": {
				"when": "nonPublicReleaseOnly",
				"where": "buildMetadata"
			}
		}
	}
}
